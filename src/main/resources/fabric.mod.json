{
	"schemaVersion": 1,
	"id": "rout",
	"version": "${version}",
	"name": "Rout",
	"description": "Mod klien sederhana yang menyediakan perintah /rout untuk menampilkan informasi mod dan mengelola alias perintah. Memungkinkan kustomisasi alias perintah dan pilihan bahasa di dalam game.",
	"authors": [
		"Ifan"
	],
	"contact": {},
	"license": "CC0-1.0",
	"icon": "assets/rout/icon.png",
	"environment": "client",
	"entrypoints": {
		"client": [
			"respawn.outbase.rout.RoutClient"
		],
		"fabric-datagen": [
			"respawn.outbase.rout.RoutDataGenerator"
		],
		"modmenu": [
			"respawn.outbase.rout.config.RoutModMenuIntegration"
		]
	},
	"mixins": [
		{
			"config": "rout.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.17.2",
		"minecraft": "~1.21.8",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"modmenu": "*"
	}
}